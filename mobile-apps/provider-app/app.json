{
  "expo": {
    "name": "TechersGPT Pro",
    "slug": "techersgpt-provider",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#10B981"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.techersgpt.provider"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#10B981"
      },
      "package": "com.techersgpt.provider"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "techersgpt-pro",
    "plugins": [
      "expo-router"
    ]
  }
}
